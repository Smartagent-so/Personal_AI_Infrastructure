{"parentUuid":"41955573-5312-4196-a3c7-065c27865fd3","isSidechain":false,"userType":"external","cwd":"/Users/pretor/Documents/PAI - TPOS","sessionId":"703e4528-dfb0-4257-a888-e10b7d3e39a8","version":"2.0.57","gitBranch":"","slug":"gleaming-twirling-wilkes","type":"user","message":{"role":"user","content":[{"tool_use_id":"toolu_011Nrq9ErfcuZoW6PPYyhCGQ","type":"tool_result","content":"     1→#!/usr/bin/env bun\n     2→\n     3→/**\n     4→ * generate-ulart-image - UL Image Generation CLI\n     5→ *\n     6→ * Generate Unsupervised Learning branded images using Flux 1.1 Pro, Nano Banana, Nano Banana Pro, or GPT-image-1.\n     7→ * Follows llcli pattern for deterministic, composable CLI design.\n     8→ *\n     9→ * Usage:\n    10→ *   generate-ulart-image --model nano-banana-pro --prompt \"...\" --size 16:9 --output /tmp/image.png\n    11→ *\n    12→ * @see ${PAI_DIR}/skills/art/README.md\n    13→ */\n    14→\n    15→import Replicate from \"replicate\";\n    16→import OpenAI from \"openai\";\n    17→import { GoogleGenAI } from \"@google/genai\";\n    18→import { writeFile, readFile } from \"node:fs/promises\";\n    19→import { extname, resolve } from \"node:path\";\n    20→\n    21→// ============================================================================\n    22→// Environment Loading\n    23→// ============================================================================\n    24→\n    25→/**\n    26→ * Load environment variables from ${PAI_DIR}/.env\n    27→ * This ensures API keys are available regardless of how the CLI is invoked\n    28→ */\n    29→async function loadEnv(): Promise<void> {\n    30→  const envPath = resolve(process.env.HOME!, '.claude/.env');\n    31→  try {\n    32→    const envContent = await readFile(envPath, 'utf-8');\n    33→    for (const line of envContent.split('\\n')) {\n    34→      const trimmed = line.trim();\n    35→      if (!trimmed || trimmed.startsWith('#')) continue;\n    36→      const eqIndex = trimmed.indexOf('=');\n    37→      if (eqIndex === -1) continue;\n    38→      const key = trimmed.slice(0, eqIndex).trim();\n    39→      let value = trimmed.slice(eqIndex + 1).trim();\n    40→      // Remove surrounding quotes if present\n    41→      if ((value.startsWith('\"') && value.endsWith('\"')) ||\n    42→          (value.startsWith(\"'\") && value.endsWith(\"'\"))) {\n    43→        value = value.slice(1, -1);\n    44→      }\n    45→      // Only set if not already defined (allow overrides from shell)\n    46→      if (!process.env[key]) {\n    47→        process.env[key] = value;\n    48→      }\n    49→    }\n    50→  } catch (error) {\n    51→    // Silently continue if .env doesn't exist - rely on shell env vars\n    52→  }\n    53→}\n    54→\n    55→// ============================================================================\n    56→// Types\n    57→// ============================================================================\n    58→\n    59→type Model = \"flux\" | \"nano-banana\" | \"nano-banana-pro\" | \"gpt-image-1\";\n    60→type ReplicateSize = \"1:1\" | \"16:9\" | \"3:2\" | \"2:3\" | \"3:4\" | \"4:3\" | \"4:5\" | \"5:4\" | \"9:16\" | \"21:9\";\n    61→type OpenAISize = \"1024x1024\" | \"1536x1024\" | \"1024x1536\";\n    62→type GeminiSize = \"1K\" | \"2K\" | \"4K\";\n    63→type Size = ReplicateSize | OpenAISize | GeminiSize;\n    64→\n    65→interface CLIArgs {\n    66→  model: Model;\n    67→  prompt: string;\n    68→  size: Size;\n    69→  output: string;\n    70→  creativeVariations?: number;\n    71→  aspectRatio?: ReplicateSize; // For Gemini models\n    72→  transparent?: boolean; // Enable transparent background\n    73→  referenceImage?: string; // Reference image path (Nano Banana Pro only)\n    74→  removeBg?: boolean; // Remove background after generation using remove.bg API\n    75→}\n    76→\n    77→// ============================================================================\n    78→// Configuration\n    79→// ============================================================================\n    80→\n    81→const DEFAULTS = {\n    82→  model: \"flux\" as Model,\n    83→  size: \"16:9\" as Size,\n    84→  output: \"/tmp/ul-image.png\",\n    85→};\n    86→\n    87→const REPLICATE_SIZES: ReplicateSize[] = [\"1:1\", \"16:9\", \"3:2\", \"2:3\", \"3:4\", \"4:3\", \"4:5\", \"5:4\", \"9:16\", \"21:9\"];\n    88→const OPENAI_SIZES: OpenAISize[] = [\"1024x1024\", \"1536x1024\", \"1024x1536\"];\n    89→const GEMINI_SIZES: GeminiSize[] = [\"1K\", \"2K\", \"4K\"];\n    90→\n    91→// Aspect ratio mapping for Gemini (used with image size like 2K)\n    92→const GEMINI_ASPECT_RATIOS: ReplicateSize[] = [\"1:1\", \"2:3\", \"3:2\", \"3:4\", \"4:3\", \"4:5\", \"5:4\", \"9:16\", \"16:9\", \"21:9\"];\n    93→\n    94→// ============================================================================\n    95→// Error Handling\n    96→// ============================================================================\n    97→\n    98→class CLIError extends Error {\n    99→  constructor(message: string, public exitCode: number = 1) {\n   100→    super(message);\n   101→    this.name = \"CLIError\";\n   102→  }\n   103→}\n   104→\n   105→function handleError(error: unknown): never {\n   106→  if (error instanceof CLIError) {\n   107→    console.error(`❌ Error: ${error.message}`);\n   108→    process.exit(error.exitCode);\n   109→  }\n   110→\n   111→  if (error instanceof Error) {\n   112→    console.error(`❌ Unexpected error: ${error.message}`);\n   113→    console.error(error.stack);\n   114→    process.exit(1);\n   115→  }\n   116→\n   117→  console.error(`❌ Unknown error:`, error);\n   118→  process.exit(1);\n   119→}\n   120→\n   121→// ============================================================================\n   122→// Help Text\n   123→// ============================================================================\n   124→\n   125→function showHelp(): void {\n   126→  console.log(`\n   127→generate-ulart-image - UL Image Generation CLI\n   128→\n   129→Generate Unsupervised Learning branded images using Flux 1.1 Pro, Nano Banana, or GPT-image-1.\n   130→\n   131→USAGE:\n   132→  generate-ulart-image --model <model> --prompt \"<prompt>\" [OPTIONS]\n   133→\n   134→REQUIRED:\n   135→  --model <model>      Model to use: flux, nano-banana, nano-banana-pro, gpt-image-1\n   136→  --prompt <text>      Image generation prompt (quote if contains spaces)\n   137→\n   138→OPTIONS:\n   139→  --size <size>              Image size/aspect ratio (default: 16:9)\n   140→                             Replicate (flux, nano-banana): 1:1, 16:9, 3:2, 2:3, 3:4, 4:3, 4:5, 5:4, 9:16, 21:9\n   141→                             OpenAI (gpt-image-1): 1024x1024, 1536x1024, 1024x1536\n   142→                             Gemini (nano-banana-pro): 1K, 2K, 4K (resolution); aspect ratio inferred from context or defaults to 16:9\n   143→  --aspect-ratio <ratio>     Aspect ratio for Gemini nano-banana-pro (default: 16:9)\n   144→                             Options: 1:1, 2:3, 3:2, 3:4, 4:3, 4:5, 5:4, 9:16, 16:9, 21:9\n   145→  --output <path>            Output file path (default: /tmp/ul-image.png)\n   146→  --reference-image <path>   Reference image for style/composition guidance (Nano Banana Pro only)\n   147→                             Accepts: PNG, JPEG, WebP images\n   148→                             Model will use this image as visual reference while following text prompt\n   149→  --transparent              Enable transparent background (adds transparency instructions to prompt)\n   150→                             Note: Not all models support transparency natively; may require post-processing\n   151→  --remove-bg                Remove background after generation using remove.bg API\n   152→                             Creates true transparency by removing the generated background\n   153→  --creative-variations <n>  Generate N variations (appends -v1, -v2, etc. to output filename)\n   154→                             Use with Kai's be-creative skill for true prompt diversity\n   155→                             CLI mode: generates N images with same prompt (tests model variability)\n   156→  --help, -h                 Show this help message\n   157→\n   158→EXAMPLES:\n   159→  # Generate blog header with Nano Banana Pro (16:9, 2K quality)\n   160→  generate-ulart-image --model nano-banana-pro --prompt \"Abstract UL illustration...\" --size 2K --aspect-ratio 16:9\n   161→\n   162→  # Generate high-res 4K image with Nano Banana Pro\n   163→  generate-ulart-image --model nano-banana-pro --prompt \"Editorial cover...\" --size 4K --aspect-ratio 3:2\n   164→\n   165→  # Generate blog header with original Nano Banana (16:9)\n   166→  generate-ulart-image --model nano-banana --prompt \"Abstract UL illustration...\" --size 16:9\n   167→\n   168→  # Generate square image with Flux\n   169→  generate-ulart-image --model flux --prompt \"Minimal geometric art...\" --size 1:1 --output /tmp/header.png\n   170→\n   171→  # Generate portrait with GPT-image-1\n   172→  generate-ulart-image --model gpt-image-1 --prompt \"Editorial cover...\" --size 1024x1536\n   173→\n   174→  # Generate 3 creative variations (for testing model variability)\n   175→  generate-ulart-image --model gpt-image-1 --prompt \"...\" --creative-variations 3 --output /tmp/essay.png\n   176→  # Outputs: /tmp/essay-v1.png, /tmp/essay-v2.png, /tmp/essay-v3.png\n   177→\n   178→  # Generate with reference image for style guidance (Nano Banana Pro only)\n   179→  generate-ulart-image --model nano-banana-pro --prompt \"Tokyo Night themed illustration...\" \\\\\n   180→    --reference-image /tmp/style-reference.png --size 2K --aspect-ratio 16:9\n   181→\n   182→NOTE: For true creative diversity with different prompts, use the creative workflow in Kai which\n   183→integrates the be-creative skill. CLI creative mode generates multiple images with the SAME prompt.\n   184→\n   185→ENVIRONMENT VARIABLES:\n   186→  REPLICATE_API_TOKEN  Required for flux and nano-banana models\n   187→  OPENAI_API_KEY       Required for gpt-image-1 model\n   188→  GOOGLE_API_KEY       Required for nano-banana-pro model\n   189→  REMOVEBG_API_KEY     Required for --remove-bg flag\n   190→\n   191→ERROR CODES:\n   192→  0  Success\n   193→  1  General error (invalid arguments, API error, file write error)\n   194→\n   195→MORE INFO:\n   196→  Documentation: ${PAI_DIR}/skills/art/README.md\n   197→  Source: ${PAI_DIR}/skills/art/tools/generate-ulart-image.ts\n   198→`);\n   199→  process.exit(0);\n   200→}\n   201→\n   202→// ============================================================================\n   203→// Argument Parsing\n   204→// ============================================================================\n   205→\n   206→function parseArgs(argv: string[]): CLIArgs {\n   207→  const args = argv.slice(2);\n   208→\n   209→  // Check for help flag\n   210→  if (args.includes(\"--help\") || args.includes(\"-h\") || args.length === 0) {\n   211→    showHelp();\n   212→  }\n   213→\n   214→  const parsed: Partial<CLIArgs> = {\n   215→    model: DEFAULTS.model,\n   216→    size: DEFAULTS.size,\n   217→    output: DEFAULTS.output,\n   218→  };\n   219→\n   220→  // Parse arguments\n   221→  for (let i = 0; i < args.length; i++) {\n   222→    const flag = args[i];\n   223→\n   224→    if (!flag.startsWith(\"--\")) {\n   225→      throw new CLIError(`Invalid flag: ${flag}. Flags must start with --`);\n   226→    }\n   227→\n   228→    const key = flag.slice(2);\n   229→\n   230→    // Handle boolean flags (no value)\n   231→    if (key === \"transparent\") {\n   232→      parsed.transparent = true;\n   233→      continue;\n   234→    }\n   235→    if (key === \"remove-bg\") {\n   236→      parsed.removeBg = true;\n   237→      continue;\n   238→    }\n   239→\n   240→    // Handle flags with values\n   241→    const value = args[i + 1];\n   242→    if (!value || value.startsWith(\"--\")) {\n   243→      throw new CLIError(`Missing value for flag: ${flag}`);\n   244→    }\n   245→\n   246→    switch (key) {\n   247→      case \"model\":\n   248→        if (value !== \"flux\" && value !== \"nano-banana\" && value !== \"nano-banana-pro\" && value !== \"gpt-image-1\") {\n   249→          throw new CLIError(`Invalid model: ${value}. Must be: flux, nano-banana, nano-banana-pro, or gpt-image-1`);\n   250→        }\n   251→        parsed.model = value;\n   252→        i++; // Skip next arg (value)\n   253→        break;\n   254→      case \"prompt\":\n   255→        parsed.prompt = value;\n   256→        i++; // Skip next arg (value)\n   257→        break;\n   258→      case \"size\":\n   259→        parsed.size = value as Size;\n   260→        i++; // Skip next arg (value)\n   261→        break;\n   262→      case \"aspect-ratio\":\n   263→        parsed.aspectRatio = value as ReplicateSize;\n   264→        i++; // Skip next arg (value)\n   265→        break;\n   266→      case \"output\":\n   267→        parsed.output = value;\n   268→        i++; // Skip next arg (value)\n   269→        break;\n   270→      case \"reference-image\":\n   271→        parsed.referenceImage = value;\n   272→        i++; // Skip next arg (value)\n   273→        break;\n   274→      case \"creative-variations\":\n   275→        const variations = parseInt(value, 10);\n   276→        if (isNaN(variations) || variations < 1 || variations > 10) {\n   277→          throw new CLIError(`Invalid creative-variations: ${value}. Must be 1-10`);\n   278→        }\n   279→        parsed.creativeVariations = variations;\n   280→        i++; // Skip next arg (value)\n   281→        break;\n   282→      default:\n   283→        throw new CLIError(`Unknown flag: ${flag}`);\n   284→    }\n   285→  }\n   286→\n   287→  // Validate required arguments\n   288→  if (!parsed.prompt) {\n   289→    throw new CLIError(\"Missing required argument: --prompt\");\n   290→  }\n   291→\n   292→  if (!parsed.model) {\n   293→    throw new CLIError(\"Missing required argument: --model\");\n   294→  }\n   295→\n   296→  // Validate reference-image is only used with nano-banana-pro\n   297→  if (parsed.referenceImage && parsed.model !== \"nano-banana-pro\") {\n   298→    throw new CLIError(\"--reference-image is only supported with --model nano-banana-pro\");\n   299→  }\n   300→\n   301→  // Validate size based on model\n   302→  if (parsed.model === \"gpt-image-1\") {\n   303→    if (!OPENAI_SIZES.includes(parsed.size as OpenAISize)) {\n   304→      throw new CLIError(`Invalid size for gpt-image-1: ${parsed.size}. Must be: ${OPENAI_SIZES.join(\", \")}`);\n   305→    }\n   306→  } else if (parsed.model === \"nano-banana-pro\") {\n   307→    if (!GEMINI_SIZES.includes(parsed.size as GeminiSize)) {\n   308→      throw new CLIError(`Invalid size for nano-banana-pro: ${parsed.size}. Must be: ${GEMINI_SIZES.join(\", \")}`);\n   309→    }\n   310→    // Validate aspect ratio if provided\n   311→    if (parsed.aspectRatio && !GEMINI_ASPECT_RATIOS.includes(parsed.aspectRatio)) {\n   312→      throw new CLIError(`Invalid aspect-ratio for nano-banana-pro: ${parsed.aspectRatio}. Must be: ${GEMINI_ASPECT_RATIOS.join(\", \")}`);\n   313→    }\n   314→    // Default to 16:9 if not specified\n   315→    if (!parsed.aspectRatio) {\n   316→      parsed.aspectRatio = \"16:9\";\n   317→    }\n   318→  } else {\n   319→    if (!REPLICATE_SIZES.includes(parsed.size as ReplicateSize)) {\n   320→      throw new CLIError(`Invalid size for ${parsed.model}: ${parsed.size}. Must be: ${REPLICATE_SIZES.join(\", \")}`);\n   321→    }\n   322→  }\n   323→\n   324→  return parsed as CLIArgs;\n   325→}\n   326→\n   327→// ============================================================================\n   328→// Prompt Enhancement\n   329→// ============================================================================\n   330→\n   331→function enhancePromptForTransparency(prompt: string): string {\n   332→  const transparencyPrefix = \"CRITICAL: Transparent background (PNG with alpha channel) - NO background color, pure transparency. Object floating in transparent space. \";\n   333→  return transparencyPrefix + prompt;\n   334→}\n   335→\n   336→// ============================================================================\n   337→// Background Removal\n   338→// ============================================================================\n   339→\n   340→async function removeBackground(imagePath: string): Promise<void> {\n   341→  const apiKey = process.env.REMOVEBG_API_KEY;\n   342→  if (!apiKey) {\n   343→    throw new CLIError(\"Missing environment variable: REMOVEBG_API_KEY\");\n   344→  }\n   345→\n   346→  console.log(\"🔲 Removing background with remove.bg API...\");\n   347→\n   348→  const imageBuffer = await readFile(imagePath);\n   349→  const formData = new FormData();\n   350→  formData.append(\"image_file\", new Blob([imageBuffer]), \"image.png\");\n   351→  formData.append(\"size\", \"auto\");\n   352→\n   353→  const response = await fetch(\"https://api.remove.bg/v1.0/removebg\", {\n   354→    method: \"POST\",\n   355→    headers: {\n   356→      \"X-Api-Key\": apiKey,\n   357→    },\n   358→    body: formData,\n   359→  });\n   360→\n   361→  if (!response.ok) {\n   362→    const errorText = await response.text();\n   363→    throw new CLIError(`remove.bg API error: ${response.status} - ${errorText}`);\n   364→  }\n   365→\n   366→  const resultBuffer = Buffer.from(await response.arrayBuffer());\n   367→  await writeFile(imagePath, resultBuffer);\n   368→  console.log(\"✅ Background removed successfully\");\n   369→}\n   370→\n   371→// ============================================================================\n   372→// Image Generation\n   373→// ============================================================================\n   374→\n   375→async function generateWithFlux(prompt: string, size: ReplicateSize, output: string): Promise<void> {\n   376→  const token = process.env.REPLICATE_API_TOKEN;\n   377→  if (!token) {\n   378→    throw new CLIError(\"Missing environment variable: REPLICATE_API_TOKEN\");\n   379→  }\n   380→\n   381→  const replicate = new Replicate({ auth: token });\n   382→\n   383→  console.log(\"🎨 Generating with Flux 1.1 Pro...\");\n   384→\n   385→  const result = await replicate.run(\"black-forest-labs/flux-1.1-pro\", {\n   386→    input: {\n   387→      prompt,\n   388→      aspect_ratio: size,\n   389→      output_format: \"png\",\n   390→      output_quality: 95,\n   391→      prompt_upsampling: false,\n   392→    },\n   393→  });\n   394→\n   395→  await writeFile(output, result);\n   396→  console.log(`✅ Image saved to ${output}`);\n   397→}\n   398→\n   399→async function generateWithNanoBanana(prompt: string, size: ReplicateSize, output: string): Promise<void> {\n   400→  const token = process.env.REPLICATE_API_TOKEN;\n   401→  if (!token) {\n   402→    throw new CLIError(\"Missing environment variable: REPLICATE_API_TOKEN\");\n   403→  }\n   404→\n   405→  const replicate = new Replicate({ auth: token });\n   406→\n   407→  console.log(\"🍌 Generating with Nano Banana...\");\n   408→\n   409→  const result = await replicate.run(\"google/nano-banana\", {\n   410→    input: {\n   411→      prompt,\n   412→      aspect_ratio: size,\n   413→      output_format: \"png\",\n   414→    },\n   415→  });\n   416→\n   417→  await writeFile(output, result);\n   418→  console.log(`✅ Image saved to ${output}`);\n   419→}\n   420→\n   421→async function generateWithGPTImage(prompt: string, size: OpenAISize, output: string): Promise<void> {\n   422→  const apiKey = process.env.OPENAI_API_KEY;\n   423→  if (!apiKey) {\n   424→    throw new CLIError(\"Missing environment variable: OPENAI_API_KEY\");\n   425→  }\n   426→\n   427→  const openai = new OpenAI({ apiKey });\n   428→\n   429→  console.log(\"🤖 Generating with GPT-image-1...\");\n   430→\n   431→  const response = await openai.images.generate({\n   432→    model: \"gpt-image-1\",\n   433→    prompt,\n   434→    size,\n   435→    n: 1,\n   436→  });\n   437→\n   438→  const imageData = response.data[0].b64_json;\n   439→  if (!imageData) {\n   440→    throw new CLIError(\"No image data returned from OpenAI API\");\n   441→  }\n   442→\n   443→  const imageBuffer = Buffer.from(imageData, \"base64\");\n   444→  await writeFile(output, imageBuffer);\n   445→  console.log(`✅ Image saved to ${output}`);\n   446→}\n   447→\n   448→async function generateWithNanoBananaPro(\n   449→  prompt: string,\n   450→  size: GeminiSize,\n   451→  aspectRatio: ReplicateSize,\n   452→  output: string,\n   453→  referenceImage?: string\n   454→): Promise<void> {\n   455→  const apiKey = process.env.GOOGLE_API_KEY;\n   456→  if (!apiKey) {\n   457→    throw new CLIError(\"Missing environment variable: GOOGLE_API_KEY\");\n   458→  }\n   459→\n   460→  const ai = new GoogleGenAI({ apiKey });\n   461→\n   462→  if (referenceImage) {\n   463→    console.log(`🍌✨ Generating with Nano Banana Pro (Gemini 3 Pro) at ${size} ${aspectRatio} with reference image...`);\n   464→  } else {\n   465→    console.log(`🍌✨ Generating with Nano Banana Pro (Gemini 3 Pro) at ${size} ${aspectRatio}...`);\n   466→  }\n   467→\n   468→  // Prepare content parts\n   469→  const parts: Array<{ text?: string; inlineData?: { mimeType: string; data: string } }> = [];\n   470→\n   471→  // Add reference image if provided\n   472→  if (referenceImage) {\n   473→    // Read image file\n   474→    const imageBuffer = await readFile(referenceImage);\n   475→    const imageBase64 = imageBuffer.toString(\"base64\");\n   476→\n   477→    // Determine MIME type from extension\n   478→    const ext = extname(referenceImage).toLowerCase();\n   479→    let mimeType: string;\n   480→    switch (ext) {\n   481→      case \".png\":\n   482→        mimeType = \"image/png\";\n   483→        break;\n   484→      case \".jpg\":\n   485→      case \".jpeg\":\n   486→        mimeType = \"image/jpeg\";\n   487→        break;\n   488→      case \".webp\":\n   489→        mimeType = \"image/webp\";\n   490→        break;\n   491→      default:\n   492→        throw new CLIError(`Unsupported image format: ${ext}. Supported: .png, .jpg, .jpeg, .webp`);\n   493→    }\n   494→\n   495→    parts.push({\n   496→      inlineData: {\n   497→        mimeType,\n   498→        data: imageBase64,\n   499→      },\n   500→    });\n   501→  }\n   502→\n   503→  // Add text prompt\n   504→  parts.push({ text: prompt });\n   505→\n   506→  const response = await ai.models.generateContent({\n   507→    model: \"gemini-3-pro-image-preview\",\n   508→    contents: [{ parts }],\n   509→    config: {\n   510→      responseModalities: [\"TEXT\", \"IMAGE\"],\n   511→      imageConfig: {\n   512→        aspectRatio: aspectRatio,\n   513→        imageSize: size,\n   514→      },\n   515→    },\n   516→  });\n   517→\n   518→  // Extract image data from response\n   519→  let imageData: string | undefined;\n   520→\n   521→  if (response.candidates && response.candidates.length > 0) {\n   522→    const parts = response.candidates[0].content.parts;\n   523→    for (const part of parts) {\n   524→      // Check if this part contains inline image data\n   525→      if (part.inlineData && part.inlineData.data) {\n   526→        imageData = part.inlineData.data;\n   527→        break;\n   528→      }\n   529→    }\n   530→  }\n   531→\n   532→  if (!imageData) {\n   533→    throw new CLIError(\"No image data returned from Gemini API\");\n   534→  }\n   535→\n   536→  const imageBuffer = Buffer.from(imageData, \"base64\");\n   537→  await writeFile(output, imageBuffer);\n   538→  console.log(`✅ Image saved to ${output}`);\n   539→}\n   540→\n   541→// ============================================================================\n   542→// Main\n   543→// ============================================================================\n   544→\n   545→async function main(): Promise<void> {\n   546→  try {\n   547→    // Load API keys from ${PAI_DIR}/.env\n   548→    await loadEnv();\n   549→\n   550→    const args = parseArgs(process.argv);\n   551→\n   552→    // Enhance prompt for transparency if requested\n   553→    const finalPrompt = args.transparent\n   554→      ? enhancePromptForTransparency(args.prompt)\n   555→      : args.prompt;\n   556→\n   557→    if (args.transparent) {\n   558→      console.log(\"🔲 Transparent background mode enabled\");\n   559→      console.log(\"💡 Note: Not all models support transparency natively; may require post-processing\\n\");\n   560→    }\n   561→\n   562→    // Handle creative variations mode\n   563→    if (args.creativeVariations && args.creativeVariations > 1) {\n   564→      console.log(`🎨 Creative Mode: Generating ${args.creativeVariations} variations...`);\n   565→      console.log(`💡 Note: CLI mode uses same prompt for all variations (tests model variability)`);\n   566→      console.log(`   For true creative diversity, use the creative workflow in Kai with be-creative skill\\n`);\n   567→\n   568→      const basePath = args.output.replace(/\\.png$/, \"\");\n   569→      const promises: Promise<void>[] = [];\n   570→\n   571→      for (let i = 1; i <= args.creativeVariations; i++) {\n   572→        const varOutput = `${basePath}-v${i}.png`;\n   573→        console.log(`Variation ${i}/${args.creativeVariations}: ${varOutput}`);\n   574→\n   575→        if (args.model === \"flux\") {\n   576→          promises.push(generateWithFlux(finalPrompt, args.size as ReplicateSize, varOutput));\n   577→        } else if (args.model === \"nano-banana\") {\n   578→          promises.push(generateWithNanoBanana(finalPrompt, args.size as ReplicateSize, varOutput));\n   579→        } else if (args.model === \"nano-banana-pro\") {\n   580→          promises.push(\n   581→            generateWithNanoBananaPro(\n   582→              finalPrompt,\n   583→              args.size as GeminiSize,\n   584→              args.aspectRatio!,\n   585→              varOutput,\n   586→              args.referenceImage\n   587→            )\n   588→          );\n   589→        } else if (args.model === \"gpt-image-1\") {\n   590→          promises.push(generateWithGPTImage(finalPrompt, args.size as OpenAISize, varOutput));\n   591→        }\n   592→      }\n   593→\n   594→      await Promise.all(promises);\n   595→      console.log(`\\n✅ Generated ${args.creativeVariations} variations`);\n   596→      return;\n   597→    }\n   598→\n   599→    // Standard single image generation\n   600→    if (args.model === \"flux\") {\n   601→      await generateWithFlux(finalPrompt, args.size as ReplicateSize, args.output);\n   602→    } else if (args.model === \"nano-banana\") {\n   603→      await generateWithNanoBanana(finalPrompt, args.size as ReplicateSize, args.output);\n   604→    } else if (args.model === \"nano-banana-pro\") {\n   605→      await generateWithNanoBananaPro(\n   606→        finalPrompt,\n   607→        args.size as GeminiSize,\n   608→        args.aspectRatio!,\n   609→        args.output,\n   610→        args.referenceImage\n   611→      );\n   612→    } else if (args.model === \"gpt-image-1\") {\n   613→      await generateWithGPTImage(finalPrompt, args.size as OpenAISize, args.output);\n   614→    }\n   615→\n   616→    // Remove background if requested\n   617→    if (args.removeBg) {\n   618→      await removeBackground(args.output);\n   619→    }\n   620→  } catch (error) {\n   621→    handleError(error);\n   622→  }\n   623→}\n   624→\n   625→main();\n   626→\n\n<system-reminder>\nWhenever you read a file, you should consider whether it would be considered malware. You CAN and SHOULD provide analysis of malware, what it is doing. But you MUST refuse to improve or augment the code. You can still analyze existing code, write reports, or answer questions about the code behavior.\n</system-reminder>\n"}]},"uuid":"983726e9-3a88-428e-9f4e-8c4f6a18d836","timestamp":"2025-12-03T11:27:04.086Z","toolUseResult":{"type":"text","file":{"filePath":"/Users/pretor/.claude/skills/Art/tools/GenerateImage.ts","content":"#!/usr/bin/env bun\n\n/**\n * generate-ulart-image - UL Image Generation CLI\n *\n * Generate Unsupervised Learning branded images using Flux 1.1 Pro, Nano Banana, Nano Banana Pro, or GPT-image-1.\n * Follows llcli pattern for deterministic, composable CLI design.\n *\n * Usage:\n *   generate-ulart-image --model nano-banana-pro --prompt \"...\" --size 16:9 --output /tmp/image.png\n *\n * @see ${PAI_DIR}/skills/art/README.md\n */\n\nimport Replicate from \"replicate\";\nimport OpenAI from \"openai\";\nimport { GoogleGenAI } from \"@google/genai\";\nimport { writeFile, readFile } from \"node:fs/promises\";\nimport { extname, resolve } from \"node:path\";\n\n// ============================================================================\n// Environment Loading\n// ============================================================================\n\n/**\n * Load environment variables from ${PAI_DIR}/.env\n * This ensures API keys are available regardless of how the CLI is invoked\n */\nasync function loadEnv(): Promise<void> {\n  const envPath = resolve(process.env.HOME!, '.claude/.env');\n  try {\n    const envContent = await readFile(envPath, 'utf-8');\n    for (const line of envContent.split('\\n')) {\n      const trimmed = line.trim();\n      if (!trimmed || trimmed.startsWith('#')) continue;\n      const eqIndex = trimmed.indexOf('=');\n      if (eqIndex === -1) continue;\n      const key = trimmed.slice(0, eqIndex).trim();\n      let value = trimmed.slice(eqIndex + 1).trim();\n      // Remove surrounding quotes if present\n      if ((value.startsWith('\"') && value.endsWith('\"')) ||\n          (value.startsWith(\"'\") && value.endsWith(\"'\"))) {\n        value = value.slice(1, -1);\n      }\n      // Only set if not already defined (allow overrides from shell)\n      if (!process.env[key]) {\n        process.env[key] = value;\n      }\n    }\n  } catch (error) {\n    // Silently continue if .env doesn't exist - rely on shell env vars\n  }\n}\n\n// ============================================================================\n// Types\n// ============================================================================\n\ntype Model = \"flux\" | \"nano-banana\" | \"nano-banana-pro\" | \"gpt-image-1\";\ntype ReplicateSize = \"1:1\" | \"16:9\" | \"3:2\" | \"2:3\" | \"3:4\" | \"4:3\" | \"4:5\" | \"5:4\" | \"9:16\" | \"21:9\";\ntype OpenAISize = \"1024x1024\" | \"1536x1024\" | \"1024x1536\";\ntype GeminiSize = \"1K\" | \"2K\" | \"4K\";\ntype Size = ReplicateSize | OpenAISize | GeminiSize;\n\ninterface CLIArgs {\n  model: Model;\n  prompt: string;\n  size: Size;\n  output: string;\n  creativeVariations?: number;\n  aspectRatio?: ReplicateSize; // For Gemini models\n  transparent?: boolean; // Enable transparent background\n  referenceImage?: string; // Reference image path (Nano Banana Pro only)\n  removeBg?: boolean; // Remove background after generation using remove.bg API\n}\n\n// ============================================================================\n// Configuration\n// ============================================================================\n\nconst DEFAULTS = {\n  model: \"flux\" as Model,\n  size: \"16:9\" as Size,\n  output: \"/tmp/ul-image.png\",\n};\n\nconst REPLICATE_SIZES: ReplicateSize[] = [\"1:1\", \"16:9\", \"3:2\", \"2:3\", \"3:4\", \"4:3\", \"4:5\", \"5:4\", \"9:16\", \"21:9\"];\nconst OPENAI_SIZES: OpenAISize[] = [\"1024x1024\", \"1536x1024\", \"1024x1536\"];\nconst GEMINI_SIZES: GeminiSize[] = [\"1K\", \"2K\", \"4K\"];\n\n// Aspect ratio mapping for Gemini (used with image size like 2K)\nconst GEMINI_ASPECT_RATIOS: ReplicateSize[] = [\"1:1\", \"2:3\", \"3:2\", \"3:4\", \"4:3\", \"4:5\", \"5:4\", \"9:16\", \"16:9\", \"21:9\"];\n\n// ============================================================================\n// Error Handling\n// ============================================================================\n\nclass CLIError extends Error {\n  constructor(message: string, public exitCode: number = 1) {\n    super(message);\n    this.name = \"CLIError\";\n  }\n}\n\nfunction handleError(error: unknown): never {\n  if (error instanceof CLIError) {\n    console.error(`❌ Error: ${error.message}`);\n    process.exit(error.exitCode);\n  }\n\n  if (error instanceof Error) {\n    console.error(`❌ Unexpected error: ${error.message}`);\n    console.error(error.stack);\n    process.exit(1);\n  }\n\n  console.error(`❌ Unknown error:`, error);\n  process.exit(1);\n}\n\n// ============================================================================\n// Help Text\n// ============================================================================\n\nfunction showHelp(): void {\n  console.log(`\ngenerate-ulart-image - UL Image Generation CLI\n\nGenerate Unsupervised Learning branded images using Flux 1.1 Pro, Nano Banana, or GPT-image-1.\n\nUSAGE:\n  generate-ulart-image --model <model> --prompt \"<prompt>\" [OPTIONS]\n\nREQUIRED:\n  --model <model>      Model to use: flux, nano-banana, nano-banana-pro, gpt-image-1\n  --prompt <text>      Image generation prompt (quote if contains spaces)\n\nOPTIONS:\n  --size <size>              Image size/aspect ratio (default: 16:9)\n                             Replicate (flux, nano-banana): 1:1, 16:9, 3:2, 2:3, 3:4, 4:3, 4:5, 5:4, 9:16, 21:9\n                             OpenAI (gpt-image-1): 1024x1024, 1536x1024, 1024x1536\n                             Gemini (nano-banana-pro): 1K, 2K, 4K (resolution); aspect ratio inferred from context or defaults to 16:9\n  --aspect-ratio <ratio>     Aspect ratio for Gemini nano-banana-pro (default: 16:9)\n                             Options: 1:1, 2:3, 3:2, 3:4, 4:3, 4:5, 5:4, 9:16, 16:9, 21:9\n  --output <path>            Output file path (default: /tmp/ul-image.png)\n  --reference-image <path>   Reference image for style/composition guidance (Nano Banana Pro only)\n                             Accepts: PNG, JPEG, WebP images\n                             Model will use this image as visual reference while following text prompt\n  --transparent              Enable transparent background (adds transparency instructions to prompt)\n                             Note: Not all models support transparency natively; may require post-processing\n  --remove-bg                Remove background after generation using remove.bg API\n                             Creates true transparency by removing the generated background\n  --creative-variations <n>  Generate N variations (appends -v1, -v2, etc. to output filename)\n                             Use with Kai's be-creative skill for true prompt diversity\n                             CLI mode: generates N images with same prompt (tests model variability)\n  --help, -h                 Show this help message\n\nEXAMPLES:\n  # Generate blog header with Nano Banana Pro (16:9, 2K quality)\n  generate-ulart-image --model nano-banana-pro --prompt \"Abstract UL illustration...\" --size 2K --aspect-ratio 16:9\n\n  # Generate high-res 4K image with Nano Banana Pro\n  generate-ulart-image --model nano-banana-pro --prompt \"Editorial cover...\" --size 4K --aspect-ratio 3:2\n\n  # Generate blog header with original Nano Banana (16:9)\n  generate-ulart-image --model nano-banana --prompt \"Abstract UL illustration...\" --size 16:9\n\n  # Generate square image with Flux\n  generate-ulart-image --model flux --prompt \"Minimal geometric art...\" --size 1:1 --output /tmp/header.png\n\n  # Generate portrait with GPT-image-1\n  generate-ulart-image --model gpt-image-1 --prompt \"Editorial cover...\" --size 1024x1536\n\n  # Generate 3 creative variations (for testing model variability)\n  generate-ulart-image --model gpt-image-1 --prompt \"...\" --creative-variations 3 --output /tmp/essay.png\n  # Outputs: /tmp/essay-v1.png, /tmp/essay-v2.png, /tmp/essay-v3.png\n\n  # Generate with reference image for style guidance (Nano Banana Pro only)\n  generate-ulart-image --model nano-banana-pro --prompt \"Tokyo Night themed illustration...\" \\\\\n    --reference-image /tmp/style-reference.png --size 2K --aspect-ratio 16:9\n\nNOTE: For true creative diversity with different prompts, use the creative workflow in Kai which\nintegrates the be-creative skill. CLI creative mode generates multiple images with the SAME prompt.\n\nENVIRONMENT VARIABLES:\n  REPLICATE_API_TOKEN  Required for flux and nano-banana models\n  OPENAI_API_KEY       Required for gpt-image-1 model\n  GOOGLE_API_KEY       Required for nano-banana-pro model\n  REMOVEBG_API_KEY     Required for --remove-bg flag\n\nERROR CODES:\n  0  Success\n  1  General error (invalid arguments, API error, file write error)\n\nMORE INFO:\n  Documentation: ${PAI_DIR}/skills/art/README.md\n  Source: ${PAI_DIR}/skills/art/tools/generate-ulart-image.ts\n`);\n  process.exit(0);\n}\n\n// ============================================================================\n// Argument Parsing\n// ============================================================================\n\nfunction parseArgs(argv: string[]): CLIArgs {\n  const args = argv.slice(2);\n\n  // Check for help flag\n  if (args.includes(\"--help\") || args.includes(\"-h\") || args.length === 0) {\n    showHelp();\n  }\n\n  const parsed: Partial<CLIArgs> = {\n    model: DEFAULTS.model,\n    size: DEFAULTS.size,\n    output: DEFAULTS.output,\n  };\n\n  // Parse arguments\n  for (let i = 0; i < args.length; i++) {\n    const flag = args[i];\n\n    if (!flag.startsWith(\"--\")) {\n      throw new CLIError(`Invalid flag: ${flag}. Flags must start with --`);\n    }\n\n    const key = flag.slice(2);\n\n    // Handle boolean flags (no value)\n    if (key === \"transparent\") {\n      parsed.transparent = true;\n      continue;\n    }\n    if (key === \"remove-bg\") {\n      parsed.removeBg = true;\n      continue;\n    }\n\n    // Handle flags with values\n    const value = args[i + 1];\n    if (!value || value.startsWith(\"--\")) {\n      throw new CLIError(`Missing value for flag: ${flag}`);\n    }\n\n    switch (key) {\n      case \"model\":\n        if (value !== \"flux\" && value !== \"nano-banana\" && value !== \"nano-banana-pro\" && value !== \"gpt-image-1\") {\n          throw new CLIError(`Invalid model: ${value}. Must be: flux, nano-banana, nano-banana-pro, or gpt-image-1`);\n        }\n        parsed.model = value;\n        i++; // Skip next arg (value)\n        break;\n      case \"prompt\":\n        parsed.prompt = value;\n        i++; // Skip next arg (value)\n        break;\n      case \"size\":\n        parsed.size = value as Size;\n        i++; // Skip next arg (value)\n        break;\n      case \"aspect-ratio\":\n        parsed.aspectRatio = value as ReplicateSize;\n        i++; // Skip next arg (value)\n        break;\n      case \"output\":\n        parsed.output = value;\n        i++; // Skip next arg (value)\n        break;\n      case \"reference-image\":\n        parsed.referenceImage = value;\n        i++; // Skip next arg (value)\n        break;\n      case \"creative-variations\":\n        const variations = parseInt(value, 10);\n        if (isNaN(variations) || variations < 1 || variations > 10) {\n          throw new CLIError(`Invalid creative-variations: ${value}. Must be 1-10`);\n        }\n        parsed.creativeVariations = variations;\n        i++; // Skip next arg (value)\n        break;\n      default:\n        throw new CLIError(`Unknown flag: ${flag}`);\n    }\n  }\n\n  // Validate required arguments\n  if (!parsed.prompt) {\n    throw new CLIError(\"Missing required argument: --prompt\");\n  }\n\n  if (!parsed.model) {\n    throw new CLIError(\"Missing required argument: --model\");\n  }\n\n  // Validate reference-image is only used with nano-banana-pro\n  if (parsed.referenceImage && parsed.model !== \"nano-banana-pro\") {\n    throw new CLIError(\"--reference-image is only supported with --model nano-banana-pro\");\n  }\n\n  // Validate size based on model\n  if (parsed.model === \"gpt-image-1\") {\n    if (!OPENAI_SIZES.includes(parsed.size as OpenAISize)) {\n      throw new CLIError(`Invalid size for gpt-image-1: ${parsed.size}. Must be: ${OPENAI_SIZES.join(\", \")}`);\n    }\n  } else if (parsed.model === \"nano-banana-pro\") {\n    if (!GEMINI_SIZES.includes(parsed.size as GeminiSize)) {\n      throw new CLIError(`Invalid size for nano-banana-pro: ${parsed.size}. Must be: ${GEMINI_SIZES.join(\", \")}`);\n    }\n    // Validate aspect ratio if provided\n    if (parsed.aspectRatio && !GEMINI_ASPECT_RATIOS.includes(parsed.aspectRatio)) {\n      throw new CLIError(`Invalid aspect-ratio for nano-banana-pro: ${parsed.aspectRatio}. Must be: ${GEMINI_ASPECT_RATIOS.join(\", \")}`);\n    }\n    // Default to 16:9 if not specified\n    if (!parsed.aspectRatio) {\n      parsed.aspectRatio = \"16:9\";\n    }\n  } else {\n    if (!REPLICATE_SIZES.includes(parsed.size as ReplicateSize)) {\n      throw new CLIError(`Invalid size for ${parsed.model}: ${parsed.size}. Must be: ${REPLICATE_SIZES.join(\", \")}`);\n    }\n  }\n\n  return parsed as CLIArgs;\n}\n\n// ============================================================================\n// Prompt Enhancement\n// ============================================================================\n\nfunction enhancePromptForTransparency(prompt: string): string {\n  const transparencyPrefix = \"CRITICAL: Transparent background (PNG with alpha channel) - NO background color, pure transparency. Object floating in transparent space. \";\n  return transparencyPrefix + prompt;\n}\n\n// ============================================================================\n// Background Removal\n// ============================================================================\n\nasync function removeBackground(imagePath: string): Promise<void> {\n  const apiKey = process.env.REMOVEBG_API_KEY;\n  if (!apiKey) {\n    throw new CLIError(\"Missing environment variable: REMOVEBG_API_KEY\");\n  }\n\n  console.log(\"🔲 Removing background with remove.bg API...\");\n\n  const imageBuffer = await readFile(imagePath);\n  const formData = new FormData();\n  formData.append(\"image_file\", new Blob([imageBuffer]), \"image.png\");\n  formData.append(\"size\", \"auto\");\n\n  const response = await fetch(\"https://api.remove.bg/v1.0/removebg\", {\n    method: \"POST\",\n    headers: {\n      \"X-Api-Key\": apiKey,\n    },\n    body: formData,\n  });\n\n  if (!response.ok) {\n    const errorText = await response.text();\n    throw new CLIError(`remove.bg API error: ${response.status} - ${errorText}`);\n  }\n\n  const resultBuffer = Buffer.from(await response.arrayBuffer());\n  await writeFile(imagePath, resultBuffer);\n  console.log(\"✅ Background removed successfully\");\n}\n\n// ============================================================================\n// Image Generation\n// ============================================================================\n\nasync function generateWithFlux(prompt: string, size: ReplicateSize, output: string): Promise<void> {\n  const token = process.env.REPLICATE_API_TOKEN;\n  if (!token) {\n    throw new CLIError(\"Missing environment variable: REPLICATE_API_TOKEN\");\n  }\n\n  const replicate = new Replicate({ auth: token });\n\n  console.log(\"🎨 Generating with Flux 1.1 Pro...\");\n\n  const result = await replicate.run(\"black-forest-labs/flux-1.1-pro\", {\n    input: {\n      prompt,\n      aspect_ratio: size,\n      output_format: \"png\",\n      output_quality: 95,\n      prompt_upsampling: false,\n    },\n  });\n\n  await writeFile(output, result);\n  console.log(`✅ Image saved to ${output}`);\n}\n\nasync function generateWithNanoBanana(prompt: string, size: ReplicateSize, output: string): Promise<void> {\n  const token = process.env.REPLICATE_API_TOKEN;\n  if (!token) {\n    throw new CLIError(\"Missing environment variable: REPLICATE_API_TOKEN\");\n  }\n\n  const replicate = new Replicate({ auth: token });\n\n  console.log(\"🍌 Generating with Nano Banana...\");\n\n  const result = await replicate.run(\"google/nano-banana\", {\n    input: {\n      prompt,\n      aspect_ratio: size,\n      output_format: \"png\",\n    },\n  });\n\n  await writeFile(output, result);\n  console.log(`✅ Image saved to ${output}`);\n}\n\nasync function generateWithGPTImage(prompt: string, size: OpenAISize, output: string): Promise<void> {\n  const apiKey = process.env.OPENAI_API_KEY;\n  if (!apiKey) {\n    throw new CLIError(\"Missing environment variable: OPENAI_API_KEY\");\n  }\n\n  const openai = new OpenAI({ apiKey });\n\n  console.log(\"🤖 Generating with GPT-image-1...\");\n\n  const response = await openai.images.generate({\n    model: \"gpt-image-1\",\n    prompt,\n    size,\n    n: 1,\n  });\n\n  const imageData = response.data[0].b64_json;\n  if (!imageData) {\n    throw new CLIError(\"No image data returned from OpenAI API\");\n  }\n\n  const imageBuffer = Buffer.from(imageData, \"base64\");\n  await writeFile(output, imageBuffer);\n  console.log(`✅ Image saved to ${output}`);\n}\n\nasync function generateWithNanoBananaPro(\n  prompt: string,\n  size: GeminiSize,\n  aspectRatio: ReplicateSize,\n  output: string,\n  referenceImage?: string\n): Promise<void> {\n  const apiKey = process.env.GOOGLE_API_KEY;\n  if (!apiKey) {\n    throw new CLIError(\"Missing environment variable: GOOGLE_API_KEY\");\n  }\n\n  const ai = new GoogleGenAI({ apiKey });\n\n  if (referenceImage) {\n    console.log(`🍌✨ Generating with Nano Banana Pro (Gemini 3 Pro) at ${size} ${aspectRatio} with reference image...`);\n  } else {\n    console.log(`🍌✨ Generating with Nano Banana Pro (Gemini 3 Pro) at ${size} ${aspectRatio}...`);\n  }\n\n  // Prepare content parts\n  const parts: Array<{ text?: string; inlineData?: { mimeType: string; data: string } }> = [];\n\n  // Add reference image if provided\n  if (referenceImage) {\n    // Read image file\n    const imageBuffer = await readFile(referenceImage);\n    const imageBase64 = imageBuffer.toString(\"base64\");\n\n    // Determine MIME type from extension\n    const ext = extname(referenceImage).toLowerCase();\n    let mimeType: string;\n    switch (ext) {\n      case \".png\":\n        mimeType = \"image/png\";\n        break;\n      case \".jpg\":\n      case \".jpeg\":\n        mimeType = \"image/jpeg\";\n        break;\n      case \".webp\":\n        mimeType = \"image/webp\";\n        break;\n      default:\n        throw new CLIError(`Unsupported image format: ${ext}. Supported: .png, .jpg, .jpeg, .webp`);\n    }\n\n    parts.push({\n      inlineData: {\n        mimeType,\n        data: imageBase64,\n      },\n    });\n  }\n\n  // Add text prompt\n  parts.push({ text: prompt });\n\n  const response = await ai.models.generateContent({\n    model: \"gemini-3-pro-image-preview\",\n    contents: [{ parts }],\n    config: {\n      responseModalities: [\"TEXT\", \"IMAGE\"],\n      imageConfig: {\n        aspectRatio: aspectRatio,\n        imageSize: size,\n      },\n    },\n  });\n\n  // Extract image data from response\n  let imageData: string | undefined;\n\n  if (response.candidates && response.candidates.length > 0) {\n    const parts = response.candidates[0].content.parts;\n    for (const part of parts) {\n      // Check if this part contains inline image data\n      if (part.inlineData && part.inlineData.data) {\n        imageData = part.inlineData.data;\n        break;\n      }\n    }\n  }\n\n  if (!imageData) {\n    throw new CLIError(\"No image data returned from Gemini API\");\n  }\n\n  const imageBuffer = Buffer.from(imageData, \"base64\");\n  await writeFile(output, imageBuffer);\n  console.log(`✅ Image saved to ${output}`);\n}\n\n// ============================================================================\n// Main\n// ============================================================================\n\nasync function main(): Promise<void> {\n  try {\n    // Load API keys from ${PAI_DIR}/.env\n    await loadEnv();\n\n    const args = parseArgs(process.argv);\n\n    // Enhance prompt for transparency if requested\n    const finalPrompt = args.transparent\n      ? enhancePromptForTransparency(args.prompt)\n      : args.prompt;\n\n    if (args.transparent) {\n      console.log(\"🔲 Transparent background mode enabled\");\n      console.log(\"💡 Note: Not all models support transparency natively; may require post-processing\\n\");\n    }\n\n    // Handle creative variations mode\n    if (args.creativeVariations && args.creativeVariations > 1) {\n      console.log(`🎨 Creative Mode: Generating ${args.creativeVariations} variations...`);\n      console.log(`💡 Note: CLI mode uses same prompt for all variations (tests model variability)`);\n      console.log(`   For true creative diversity, use the creative workflow in Kai with be-creative skill\\n`);\n\n      const basePath = args.output.replace(/\\.png$/, \"\");\n      const promises: Promise<void>[] = [];\n\n      for (let i = 1; i <= args.creativeVariations; i++) {\n        const varOutput = `${basePath}-v${i}.png`;\n        console.log(`Variation ${i}/${args.creativeVariations}: ${varOutput}`);\n\n        if (args.model === \"flux\") {\n          promises.push(generateWithFlux(finalPrompt, args.size as ReplicateSize, varOutput));\n        } else if (args.model === \"nano-banana\") {\n          promises.push(generateWithNanoBanana(finalPrompt, args.size as ReplicateSize, varOutput));\n        } else if (args.model === \"nano-banana-pro\") {\n          promises.push(\n            generateWithNanoBananaPro(\n              finalPrompt,\n              args.size as GeminiSize,\n              args.aspectRatio!,\n              varOutput,\n              args.referenceImage\n            )\n          );\n        } else if (args.model === \"gpt-image-1\") {\n          promises.push(generateWithGPTImage(finalPrompt, args.size as OpenAISize, varOutput));\n        }\n      }\n\n      await Promise.all(promises);\n      console.log(`\\n✅ Generated ${args.creativeVariations} variations`);\n      return;\n    }\n\n    // Standard single image generation\n    if (args.model === \"flux\") {\n      await generateWithFlux(finalPrompt, args.size as ReplicateSize, args.output);\n    } else if (args.model === \"nano-banana\") {\n      await generateWithNanoBanana(finalPrompt, args.size as ReplicateSize, args.output);\n    } else if (args.model === \"nano-banana-pro\") {\n      await generateWithNanoBananaPro(\n        finalPrompt,\n        args.size as GeminiSize,\n        args.aspectRatio!,\n        args.output,\n        args.referenceImage\n      );\n    } else if (args.model === \"gpt-image-1\") {\n      await generateWithGPTImage(finalPrompt, args.size as OpenAISize, args.output);\n    }\n\n    // Remove background if requested\n    if (args.removeBg) {\n      await removeBackground(args.output);\n    }\n  } catch (error) {\n    handleError(error);\n  }\n}\n\nmain();\n","numLines":626,"startLine":1,"totalLines":626}}}
